{
    "env": {
        "PATH": "$(PATH):$(HOME)\/.local\/bin"
    },
    "apps": [
        {
            "name": "Steam - 1280x800",
            "detached": [
                "flatpak-spawn --host setsid steam steam:\/\/open\/bigpicture"
            ],
            "image-path": "",
            "prep-cmd": [
                {
                    "do": "kscreen-doctor output.DP-1.mode.1280x800@60",
                    "undo": "kscreen-doctor output.DP-1.mode.3440x1440@144"
                }
            ],
            "exclude-global-prep-cmd": "false"
        },
        {
            "name": "Steam - 1920x1080",
            "detached": [
                "flatpak-spawn --host setsid steam steam:\/\/open\/bigpicture"
            ],
            "image-path": "",
            "prep-cmd": [
                {
                    "do": "kscreen-doctor output.DP-1.mode.1920x1080@60",
                    "undo": "kscreen-doctor output.DP-1.mode.3440x1440@144"
                }
            ],
            "exclude-global-prep-cmd": "false"
        },
        {
            "name": "Desktop - 1280x800",
            "output": "",
            "cmd": "",
            "exclude-global-prep-cmd": "false",
            "elevated": "false",
            "prep-cmd": [
                {
                    "do": "kscreen-doctor output.DP-1.mode.1280x800@60",
                    "undo": "kscreen-doctor output.DP-1.mode.3440x1440@144"
                }
            ],
            "image-path": ""
        },
        {
            "name": "Desktop - 1920x1080",
